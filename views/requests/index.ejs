
<%- include('../partials/header') %>


<h1>All Requests</h1>

<table class="request-lists">
      <% requests.forEach(function(r) { %>
        <div><%= r.userName %></div>
        <div><%= r.requestType %></div>
        <% if (r.date) { %> 
        <div>Service Date: <%= r.date %></div>
        <% } %> 
        <div><%= r.content %></div>
        <div><a href="/requests/<%= r._id %>">VIEW</a></div>
        <br>
      <% }); %>